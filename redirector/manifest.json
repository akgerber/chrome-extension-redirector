{
  "name": "Buttylitics",
  "description": "Reset Google Analytics UTM variables to mybutt",
  "version": "1.0",
  "manifest_version": 3,
  "background": {"scripts":["URI/src/URI.js", "background.js"]},
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.bootstrapcdn.com; object-src 'self'"
}
